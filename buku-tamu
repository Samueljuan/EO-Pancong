    <script>
        var loaderApp = new Vue({
            el: "#loader",
            data: {
                isLoading: false,
            },
            methods: {
                loading: function() {
                    this.isLoading = true;
                },
                stopLoading: function() {
                    this.isLoading = false;
                }
            }
        });

    </script>
    
    <script type="text/javascript">
        var mainApp = new Vue({
            el: "#main-app",
            data: {
                auth:  false ,
                success: false,
                errors: {},
                newmsg: {},
                old_transaction: null,
                nextStep: false,
                subscribe_button: 'register',
                show_password: false,
                register_data: {},
                login_data: {},
                pop_up_process: 1,
                pop_up_message: '...'
            },
            methods: {
                // askToSubscribe: function(){
                //     if(!this.auth){
                //         var show_alert = false;
                //         if(!localStorage.getItem('subscription_data')){
                //             localStorage.setItem('subscription_data', JSON.stringify({
                //                 status: false,
                //                 alert: true,
                //                 alert_time: new Date().getTime()
                //             }));
                //             show_alert = true;
                //         } 
                //         var subscription = JSON.parse(localStorage.getItem('subscription_data'));
                //         if(!subscription.status && subscription.alert){
                //             show_alert = true;
                //         }
                        
                //         if(show_alert){
                //             var timeout = 3000 //miliseconds;
                //             var now = new Date().getTime();
                //             if(subscription.alert_time > now){
                //                 timeout = subscription.alert_time - now;
                //             }                            
                //             setTimeout(function(){
                //                 $("#subscription-modal").modal({backdrop: 'static', keyboard: false})  ;
                //             }, timeout);
                //         }
                //     }
                // },
                // askToSubscribeLater: function(){
                //     localStorage.setItem('subscription_data', JSON.stringify({
                //         status: false,
                //         alert: true,
                //         alert_time: new Date().getTime()+3*60*1000
                //     }));
                //     $("#subscription-modal").modal('hide');
                //     this.askToSubscribe();
                // },
                // askToSubscribeNever: function(){
                //     localStorage.setItem('subscription_data', JSON.stringify({
                //         status: false,
                //         alert: false,
                //         alert_time: new Date().getTime()
                //     }));
                //     $("#subscription-modal").modal('hide');
                // },
                confirmPayment: function() {
                    window.location = "http://wangsit.co.id/transaction/confirm?order_id="+JSON.parse(localStorage.old_transaction).order_id;
                },
                copyAmount: function(){
                    var copyText = document.getElementById("copy_amount");
                    copyText.select();
                    document.execCommand("copy");
                    alert("Copied : " + copyText.value);
                },
                doRegister: function(){
                    var _this = this;
                    loaderApp.loading();
                    axios.post("http://wangsit.co.id/popup/register", this.register_data)
                    .then(function(response){
                        if(response.data.success == true){
                            _this.pop_up_message = response.data.message;  
                            _this.pop_up_process ++;   
                            _this.errors = {};      
                            if(_this.register_data.subscribe){
                                window.localStorage.setItem('subscription_data', JSON.stringify({
                                    status: true,
                                    alert: false,
                                    alert_time: new Date().getTime()
                                }));
                            }               
                        }
                        loaderApp.stopLoading();
                    })
                    .catch(function(error){
                        console.log(error.response);
                        _this.errors = error.response.data.errors;
                        loaderApp.stopLoading();
                    })
                },
                redirect: function (url) {
                    window.location = url;
                },
                newTab: function (url) {
                    var win = window.open(url, '_blank');
                    win.focus();
                },
                sendWA: function(message) {
                                        if (/Android|iPhone|iPad|iPod|BlackBerry|IEMobile|Opera Mini/i.test(navigator.userAgent)) {
                        var whatsapp_url =
                            "https://api.whatsapp.com/send?phone=" + message;
                        window.open(whatsapp_url, '_blank');
                    } else {
                        // alert('you are not using mobile device.');
                        var whatsapp_url =
                            "https://web.whatsapp.com/send?phone=" + message;
                        window.open(whatsapp_url, '_blank');
                    }
                    ev_track('LEAD', {
                        event: 'send wa',
                        data: {
                            message: message
                        },
                        source: window.location.pathname
                    })
                },
                sharePostWA: function() {
                                        if (/Android|iPhone|iPad|iPod|BlackBerry|IEMobile|Opera Mini/i.test(navigator.userAgent)) {
                        var whatsapp_url =
                        "https://api.whatsapp.com/send?text=" + window.location.href;
                        window.open(whatsapp_url, '_blank');
                    } else {
                        // alert('you are not using mobile device.');
                        var whatsapp_url =
                            "https://web.whatsapp.com/send?text=" + window.location.href;
                            window.open(whatsapp_url, '_blank');
                        }
                        ev_track('SHARE', {
                        event: 'share link',
                        data: {
                            message: 'Share Link '+ window.location.href + ' menggunakan Whatsapp'
                        },
                        source: window.location.pathname
                    })
                },
                sharePostLine: function() {
                                            var line_url = "https://lineit.line.me/share/ui?url="+ window.location.href;
                        window.open(line_url, '_blank');
                        ev_track('SHARE', {
                        event: 'share link Line',
                        data: {
                            message: 'Share Link '+ window.location.href +' menggunakan Line'
                        },
                        source: window.location.pathname
                    })
                },
                sharePostFB: function() {
                                            var fb_url = "https://www.facebook.com/sharer/sharer.php?u="+ window.location.href +"&display=popup";
                        window.open(fb_url, '_blank');
                        ev_track('SHARE', {
                        event: 'share link Facebook',
                        data: {
                            message: 'Share Link'+ window.location.href +' menggunakan Facebook'
                        },
                        source: window.location.pathname
                    })
                },
                cancelOrder: function(){
                    var c = confirm('Are you sure to cancel the order?');
                    if (c) {
                        axios.get("http://wangsit.co.id/order/cancel/?oi=" + JSON.parse(localStorage.old_transaction).order_id)
                            .then(function(response){
                                if (response.data.status == 'order canceled') {
                                    localStorage.removeItem('old_transaction');
                                    $('#oldTransactionModal').modal('hide');
                                } else {
                                    console.log(response.data.status);

                                }
                            })
                            .catch(function(error){
                                console.log(error.response);
                            })
                    }
                },
                createMsg: function () {
                    var _this = this;
                    axios.post("http://wangsit.co.id/add_message", this.newmsg).then(function(response){
                            _this.newmsg.name = null;
                            _this.newmsg.email = null;
                            _this.newmsg.message = null;
                            _this.errors = null;
                            _this.success = true;
                            setTimeout(function(){
                                _this.success = false;
                            }, 3000);
                        })
                        .catch(function(error){
                            _this.errors = error.response.data.errors;
                            console.log(error.response.data);
                        });
                },
                getError: function(label) {
                    if (this.errors) {
                        if (label in this.errors) {
                            return this.errors[label][0];
                        } else {
                            return null;
                        }
                    } else {
                        return null;
                    }
                },
                checkOldTransaction: function(orderId) {
                    var _this = this;
                    axios.get("http://wangsit.co.id/order/status/?oi=" + orderId)
                        .then(function(response){
                            console.log(response.data);

                            if (response.data.status == 'paid') {
                                localStorage.removeItem('old_transaction');
                            } else if (response.data.status == 'order') {
                                _this.old_transaction = JSON.parse(localStorage.old_transaction).newtransaction;
                                $('#oldTransactionModal').modal('show');
                            } else {
                                console.log(response.data.status);
                            }
                        })
                        .catch(function(error) {
                            console.log(error.response.data);
                        })
                },
                finishPayment: function() {
                    var old = JSON.parse(localStorage.old_transaction);
                    if (old.newtransaction.payment == 'BCA') {
                        this.nextStep = true;

                    } else {
                        this.nextStep = false;
                        window.location = old.confirm_url;
                    }
                }
            },
            mounted: function() {
                if(window.location.pathname != '/transaction/confirm'){
                    if (localStorage.getItem('old_transaction')) {
                        var old = JSON.parse(localStorage.old_transaction);
                        if (new Date(old.exp) <= new Date()) {
                            localStorage.removeItem('old_transaction');
                        } else {
                            this.checkOldTransaction(old.order_id)
                        }
                    }
                }
                // this.askToSubscribe();

                // this.autoCompletePhone("#register_phone");
                // this.autoCompletePhone("#login_phone");
            }
        });

    </script>
