    <script>
        var navApp = new Vue({
            el: "#navApp",
            data: {
                notifications: [],
                query: '',
                results: []
            },
            methods: {
                                // autoComplete: function() {
                //     if(this.query.length > 2){
                //     axios.get('search',{params: {query: this.query}}).then(response => {
                //     this.results = response.data;
                //     });
                // }
            },
            computed: {
                notif_count: function() {
                    return this.notifications.length;
                }
            },
            mounted: function() {
                if(!localStorage.getItem('deviceId')){
                    localStorage.setItem('deviceId', generateRandomString(10));
                }
                if(!sessionStorage.getItem('sessionId')){
                    sessionStorage.setItem('sessionId', generateRandomString(10));
                }
                axios.post(window.location.origin+'/api/v1/guest/analytic.track-user-active', {
                    device_id: localStorage.getItem('deviceId'),
                    type: 'web'
                })
                                
            },

        });

        

        $(function () {
            $('#modalNotif').on('shown.bs.modal', function () {
                $('.collapse').collapse('hide');
            })
            var current_url = "http://wangsit.co.id/buku-tamu";
            var x = $(".dropdown-menu > [href='" + current_url + "']").parent().parent().find(
                "a.dropdown-toggle").addClass('active');
            // console.log(x);
            $("[href='" + current_url + "']").addClass('active');
            $('.navbar-toggler').click(function (e) {
                $('.collapse').collapse('hide');
            });
        });

        $(function () {
            $(".fab,.backdrop").click(function () {
                if ($(".backdrop").is(":visible")) {
                    $(".backdrop").fadeOut(125);
                    $(".fab.child")
                        .stop()
                        .animate({
                            bottom: $("#masterfab").css("bottom"),
                            opacity: 0
                        }, 125, function () {
                            $(this).hide();
                        });
                } else {
                    $(".backdrop").fadeIn(125);
                    $(".fab.child").each(function () {
                        $(this)
                            .stop()
                            .show()
                            .animate({
                                bottom: (parseInt($("#masterfab").css("bottom")) +
                                    parseInt(
                                        $("#masterfab").outerHeight()) + 70 * $(
                                        this).data(
                                        "subitem") - $(".fab.child").outerHeight()
                                ) + "px",
                                opacity: 1
                            }, 125);
                    });
                }
            });
        });
        function generateRandomString(length) {
            var text = "";
            var possible = "ABCDEFGHIJKLMNOPQRSTUVWXYZabcdefghijklmnopqrstuvwxyz0123456789";
            
            for (var i = 0; i < length; i++)
                text += possible.charAt(Math.floor(Math.random() * possible.length));
            
            return text;
        }
    </script>

    <script>

  Vue.component('auto-complete', {
    template:`
    <div>
   
      <input type="text" placeholder="Search" v-model="query" @keyup="getData()" autocomplete="off" class="form-control form-control-sm" />
      <div v-if="query.length > 1">
      <div class="panel-footer" v-if="search_data.length">
        <ul class="list-group" >
          <a :href="'http://wangsit.co.id/vendor-directory/'+data1.name+'/page'" v-for="data1 in search_data" class="list-group-item" @click="getName(data1.name)">{{ data1.name }}</a>
        </ul>
      </div>
    </div>
    `,
    data:function(){
      return{
        query:"",
        search_data:[]
      }
    },
    methods:{
      getData:function(){
        if(this.query.length > 2){
        this.search_data = [];
        axios.get("search?query="+this.query).then(response => {
          this.search_data = response.data;
        });
    }
      },
      getName:function(name){
        this.query = name;
        this.search_data = [];
      }
    }
  });

  var application = new Vue({
    el:"#autocomplete_app"
  });

</script>
