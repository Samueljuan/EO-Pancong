    <script>
    function ev_track(type, data) {
        axios.post("http://wangsit.co.id/track/" + type, {
                data: data,
                identity: {
                    user_agent: navigator.userAgent,
                    referrer: document.referrer,
                }
            })
            .catch(function(error){
                console.log(error.response);
            });
    }
    ev_track('page_view', {
        path: window.location.pathname
    })
    setInterval(function(){
        ev_track('idle', {path: window.location.pathname})
    }, 1800000)

</script>
